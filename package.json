{
    "name": "mcphr-browser",
    "version": "1.0.0",
    "description": "Mountain Care HR App - Browser-based using sql.js, with future PostgreSQL support.",
    "main": "server/index.js",
    "browser": {
        "fs": false,
        "path": false,
        "os": false,
        "vm": false
    },
    "scripts": {
        "start:client": "craco start",
        "start:server": "nodemon server/index.js",
        "start": "concurrently \"npm run start:server\" \"npm run start:client\"",
        "build": "craco build",
        "postbuild": "cp -r node_modules/sql.js/dist/*.wasm build/static/js/",
        "vercel-build": "craco build && cp -r node_modules/sql.js/dist/*.wasm build/static/js/",
        "test": "craco test --env=jsdom",
        "lint": "eslint ."
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^6.7.2",
        "@fortawesome/free-solid-svg-icons": "^6.7.2",
        "assert": "^2.1.0",
        "browserify-zlib": "^0.2.0",
        "cors": "^2.8.5",
        "express": "^4.18.2",
        "https-browserify": "^1.0.0",
        "idb": "^8.0.2",
        "jsonwebtoken": "^8.5.1",
        "path-browserify": "^1.0.1",
        "pg": "^8.9.0",
        "querystring-es3": "^0.2.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^7.4.0",
        "sequelize": "^6.28.0",
        "sql.js": "^1.13.0",
        "stream-http": "^3.2.0",
        "stream-browserify": "^3.0.0",
        "url": "^0.11.4",
        "util": "^0.12.5",
        "vm-browserify": "^1.1.2",
        "web-vitals": "^4.2.4",
        "buffer": "^6.0.3",
        "crypto-browserify": "^3.12.1"
    },
    "devDependencies": {
        "@craco/craco": "^7.0.0",
        "@types/jsonwebtoken": "^9.0.9",
        "@types/node": "^18.0.0",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "@types/sql.js": "^1.4.9",
        "concurrently": "^7.0.0",
        "eslint": "^8.0.0",
        "nodemon": "^2.0.20",
        "react-scripts": "^5.0.1",
        "wasm-loader": "^1.3.0"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "author": "Mountain Care Pharmacy",
    "license": "MIT",
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}